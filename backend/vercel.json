{
    "version": 2,
    "builds": [
        {
            "src": "app.js",  // Main entry point for the application
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"  // Ensure this points to the correct route handling file
        }
    ]
}
